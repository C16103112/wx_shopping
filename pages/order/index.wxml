<Tabs tabs="{{tabs}}" bindtabsItemChange="handleTabsItemChange">
	<view class="order_main">
		<view wx:for="{{order}}" class="order_item">
			<view class="order_no_row">
				<view class="roder_no_text">订单编号</view>
				<view class="roder_no_value">{{item.goods_id}}</view>
			</view>
			<view class="order_price_row">
				<view class="order_price_text">订单价格</view>
				<view class="order_price_value">￥{{item.num * item.goods_price}}</view>
			</view>
			<view class="order_time_row">
				<view class="order_time_text">订单日期</view>
				<view class="order_time_value">{{item.time}}</view>
			</view>
		</view>
	</view>
</Tabs>

